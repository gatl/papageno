   #ifdef PPG_CS_COMPRESS_CONTEXT
   ppg_compression_run(ccontext, "test");
   #endif
   
   #ifndef PPG_CS_SUPPRESS_TESTS
   
   #include @PPG_CS_COMPRESSION_TESTS_INCLUDE_FILE@
   
   #endif // ifndef PPG_CS_SUPPRESS_TESTS
   
